{
  "name": "react-poc",
  "version": "0.1.0",
  "private": true,
  "description": "App POC ReactJS with JS",
  "keywords": [
    "poc",
    "react",
    "reactjs"
  ],
  "files": [
    "build"
  ],
  "homepage": "https://krbaio3.github.io/react-poc/",
  "bugs": {
    "url": "https://github.com/krbaio3/react-poc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krbaio3/react-poc.git"
  },
  "license": "MIT",
  "author": "krbaio3 <krbaio3@gmail.com>",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "predeploy:gh": "yarn build",
    "deploy:gh": "gh-pages -d build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "ci:login": "firebase login --token $FIREBASE_TOKEN -y",
    "ci:deploy": "firebase deploy --token $FIREBASE_TOKEN"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5"
  },
  "devDependencies": {
    "animate.css": "^3.7.0",
    "animejs": "^3.0.1",
    "eslint": "5.15.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "firebase": "^5.9.0"
  }
}
